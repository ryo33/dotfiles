urxvt.saveLines: 512
urxvt.scrollBar: false
urxvt.termName: screen-256color

! color
urxvt.foreground: #93a1a1
urxvt.background: #002b36

urxvt*font: xft:mono:pixelsize=16:antialias=true:hinting=true
urxvt*boldFont: xft:mono:pixelsize=16:antialias=true:bold:hinting=true
urxvt*italicFont: xft:mono:pixelsize=16:antialias=true:italic:autohint=true:hinting=true
urxvt*bolditalicFont: xft:mono:pixelsize=16:antialias=true:bold:italic:autohint=true:hinting=true

urxvt.keysym.M-minus: command:\000

urxvt.lineSpace: 1

!! black dark/light
! urxvt.color0:                #073642
! urxvt.color8:                #002b36

!! red dark/light
urxvt.color1:                #dc322f
urxvt.color9:                #cb4b16

!! green dark/light
urxvt.color2:                #859900
urxvt.color10:               #859900

!! yellow dark/light
urxvt.color3:                #b58900
urxvt.color11:               #b58900

!! blue dark/light
urxvt.color4:                #268bd2
urxvt.color12:               #268bd2

!! magenta dark/light
urxvt.color5:                #d33682
urxvt.color13:               #6c71c4

!! cyan dark/light
urxvt.color6:                #2aa198
urxvt.color14:               #2aa198

!! white dark/light
! urxvt.color7:                #eee8d5
! urxvt.color15:               #fdf6e3
